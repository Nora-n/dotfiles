if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 87
normal! 06|
wincmd w
argglobal
if bufexists(fnamemodify("~/.config/texmf/tex/latex/local/custikz.sty", ":p")) | buffer ~/.config/texmf/tex/latex/local/custikz.sty | else | edit ~/.config/texmf/tex/latex/local/custikz.sty | endif
if &buftype ==# 'terminal'
  silent file ~/.config/texmf/tex/latex/local/custikz.sty
endif
